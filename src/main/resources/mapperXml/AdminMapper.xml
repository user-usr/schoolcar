<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.GOTEL.SchoolCar.Mapper.AdminMapper">
    <select id="getAll" resultType="com.GOTEL.SchoolCar.Entity.Admin">
        select *
        from admin
    </select>

    <select id="getByAID" resultType="com.GOTEL.SchoolCar.Entity.Admin">
        select *
        from admin
        where AID = #{AID}
    </select>


    <select id="getByJobNumber" resultType="com.GOTEL.SchoolCar.Entity.Admin">
        select *
        from admin
        where A_JobNumber = #{jobNumber}
    </select>

    <select id="getAdminNumber">
        select count(admin.AID)
        from admin
    </select>
</mapper>
